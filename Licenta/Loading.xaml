<Window x:Class="Licenta.Loading"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Licenta"
        mc:Ignorable="d"
        Title="Loading" Height="400" Width="700" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Image HorizontalAlignment="Left" Height="400" VerticalAlignment="Top" Width="700" Stretch="Fill" Source="GRABOBLOCK SPLASH.jpg"/>
        <ProgressBar x:Name="pb" HorizontalAlignment="Left" Height="12" Margin="0,388,0,0" VerticalAlignment="Top" Width="700" ValueChanged="ProgressBar_ValueChanged"/>

    </Grid>
</Window>
